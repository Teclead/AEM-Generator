<ELNAME jcr:primaryType="nt:unstructured"
     sling:resourceType="granite/ui/components/foundation/fileupload"
     reactImageEditor="reactImageEditorHandler(this)"
     autoStart="{Boolean}false"
     class="cq-droptarget"
     fileNameParameter="./fileName"
     fileReferenceParameter="{{DATABASE}}"
     mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff]"
     multiple="{Boolean}false"
     name="./file"
     fieldLabel="{{TITLE}}"
     uploadUrl="${suffix.path}"
     allowUpload="false"
     useHTML5="{Boolean}true" {{DISABLED}} {{DESC}} {{REQUIRED}}/>